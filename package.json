    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "dotenv": "^8.2.0",
    "json-server": "^0.16.3",
    "nodemon": "^2.0.7",
    "ra-data-json-server": "^3.13.0",
    "ra-data-simple-rest": "^3.10.4",
    "react": "^17.0.1",
    "react-admin": "^3.13.0",
